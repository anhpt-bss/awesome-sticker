{
  "name": "awesome-sticker",
  "version": "1.1.3",
  "private": false,
  "description": "A awesome sticker for chat application",
  "author": "AnhPT",
  "license": "MIT",
  "keywords": [
    "awesome",
    "sticker",
    "reaction",
    "chat",
    "popover"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anhpt-bss/awesome-sticker.git"
  },
  "bugs": {
    "url": "https://github.com/anhpt-bss/awesome-sticker/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "publish:npm": "(if exist dist rmdir /s /q dist) && mkdir dist && babel ./src/containers -d dist --copy-files"
  },
  "dependencies": {
    "@ladifire-opensource/stylex": "^0.1.0-beta.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@craco/craco": "^7.1.0",
    "@ladifire-opensource/babel-plugin-transform-stylex": "^0.1.0-beta.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@ladifire-opensource/babel-plugin-transform-stylex",
        {
          "inject": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
